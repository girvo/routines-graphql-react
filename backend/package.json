{
  "name": "@my-routines/backend",
  "packageManager": "pnpm@10.21.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:introspect": "node --env-file=.env tools/introspect-db.ts",
    "migration:down": "node --env-file=.env tools/run-migrations.ts down",
    "migration:latest": "node --env-file=.env tools/run-migrations.ts latest",
    "migration:make": "node --env-file=.env tools/make-migration.ts",
    "migration:up": "node --env-file=.env tools/run-migrations.ts up",
    "resolvers:generate": "graphql-codegen",
    "start": "node --env-file=.env --watch --watch-preserve-output src/main.ts",
    "test": "node --env-file=.env.test ./node_modules/vitest/vitest.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@envelop/dataloader": "^9.0.0",
    "@envelop/generic-auth": "^10.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.3.0",
    "@graphql-yoga/plugin-jwt": "^3.10.0",
    "arktype": "^2.1.25",
    "bcryptjs": "3.0.3",
    "better-sqlite3": "^12.4.1",
    "dataloader": "^2.2.3",
    "date-fns": "^4.1.0",
    "fastify": "^5.6.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.25.0",
    "graphql-yoga": "^5.15.2",
    "jsonwebtoken": "9.0.2",
    "kysely": "^0.28.8",
    "pino-pretty": "^13.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typed-document-node": "^6.1.4",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-resolvers": "^5.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsonwebtoken": "9.0.10",
    "glob": "^13.0.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.14"
  }
}
